{
	"name": "workers-logger",
	"version": "0.2.0",
	"description": "A fast and effective logging framework for Cloudflare Workers",
	"keywords": [
		"logging",
		"tracing",
		"workers"
	],
	"repository": "maraisr/workers-logger",
	"license": "MIT",
	"author": "Marais Rossow <me@marais.dev> (https://marais.io)",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js",
			"types": "./index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "index.js",
	"module": "index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.js",
		"index.mjs",
		"index.d.ts"
	],
	"scripts": {
		"build": "bundt --minify",
		"format": "prettier  --write --list-different \"{*,{src,examples}/**/*,.github/**/*}.+(ts|json|yml|md)\"",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"diary": "^0.4.4"
	},
	"devDependencies": {
		"@marais/tsconfig": "0.0.4",
		"bundt": "2.0.0-next.5",
		"prettier": "2.8.8",
		"typescript": "5.1.3"
	},
	"volta": {
		"node": "18.16.1"
	}
}
