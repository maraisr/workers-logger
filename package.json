{
	"name": "workers-logger",
	"version": "0.0.4",
	"description": "A fast and effective logging framework for Cloudflare Workers",
	"keywords": [
		"logging",
		"tracing",
		"workers"
	],
	"repository": "maraisr/workers-logger",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "index.js",
	"module": "index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.js",
		"index.mjs",
		"index.d.ts"
	],
	"scripts": {
		"build": "rollup -c",
		"fmt": "prettier  --write --list-different \"{*,{src,examples}/**/*,.github/**/*}.+(ts|json|yml|md)\"",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"diary": "^0.2.1"
	},
	"volta": {
		"node": "16.2.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "2.2.2",
		"@marais/tsconfig": "0.0.1",
		"prettier": "2.4.0",
		"rollup": "2.56.3",
		"rollup-plugin-dts": "4.0.0",
		"typescript": "4.4.3"
	}
}
